msgctxt "model:ir.message,text:msg_cancel_invoice_with_move_post"
msgid "You cannot cancel an invoice with a posted move."
msgstr "No se puede cancelar una factura  con un asiento contabilizado."

msgctxt "model:ir.message,text:msg_cancel_move_with_origin"
msgid ""
"The move \"%(move)s\" colud not be cancelled because came from origin "
"\"%(origin)s\". To cancel it has to be done througth the origin."
msgstr ""
"El asiento \"%(move)s\" no se puede cancelar porque tiene un origen "
"\"%(origin)s\". Para cancelarlo se debe hacer desde el origen."

msgctxt "model:ir.message,text:msg_payment_different_process_method"
msgid ""
"Payment process method can not be mixed on payment groups. Payment process "
"\"%(process)s\" of line \"%(payment)s\" is diferent from previous payment "
"process \"%(pprocess)s\""
msgstr ""
"El método del proceso de pago no se puede mezclar en grupos de pago. El "
"proceso de pago \"%(process)s\" de la línea \"%(payment)s\" es diferente del"
" proceso de pago anterior \"%(pprocess)s\""

msgctxt "model:ir.message,text:msg_payment_non_posted_move"
msgid ""
"You can not pay line \"%(line)s\" because its move \"%(move)s\" is not "
"posted.'"
msgstr ""
"No puede pagar la línea \"%(line)s\" porque su asiento \"%(move)s\" no está "
"contabilizado.'"

